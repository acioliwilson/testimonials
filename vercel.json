{
 "version": 2,
 "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
 ],
 "routes": [
    {
      "src": "/api/v1/testimonials",
      "methods": ["GET", "POST"],
      "dest": "index.js"
    },
    {
        "src": "/api/v1/testimonials/(.*)",
        "methods": ["PUT", "DELETE"],
        "dest": "index.js"
    }
 ]
}